<!DOCTYPE html>
<html lang="ja">
  <%= render 'layouts/rails_default' %>

  <body>
  <%= render 'layouts/header' %>
  <div class="container">
    <% flash.each do |msg_type, msg| %>
      <%= content_tag(:div, msg, class: "alert alert-#{msg_type}") %>
    <% end %>
    <%= yield %>
    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
  </div>
  </body>
</html>
